{
	"name": "@caeljs/ejb",
	"type": "module",
	"description": "A simple view system inspired by Blade.",
	"version": "0.1.0",
	"main": "cjs/index.cjs",
	"module": "esm/index.js",
	"types": "index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/drylian/configjs.git"
	},
	"typesVersions": {
		"*": {
			".": [
				"./index.d.ts"
			]
		}
	},
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./esm/index.js",
			"require": "./cjs/index.cjs"
		}
	},
	"keywords": [
		"backend",
		"standardization",
		"standard",
		"blade",
		"npm",
		"node",
		"bun",
		"deno",
		"typescript",
		"debug",
		"output",
		"performance"
	],
	"author": "Drylian",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/drylian/ejb/issues"
	},
	"homepage": "https://github.com/drylian/ejb#readme",
	"scripts": {
		"build": "bun run build.ts",
		"test": "bun test",
		"ejbconfig": "bun run build.ts --only-ejbconfig",
		"check": "bunx biome check src/** --write"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/bun": "^1.2.22",
		"@types/node": "^24.5.2",
		"dts-bundle-generator": "^9.5.1",
		"tsup": "^8.5.0",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	}
}
